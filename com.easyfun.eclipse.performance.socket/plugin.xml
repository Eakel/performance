<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="com.easyfun.eclipse.performance.EasyFunNavigatorFolder">
      <folder
            id="com.easyfun.eclipse.performance.socketFolder"
            title="Socket"
            type="Socket"
            visible="false">
      </folder>
   </extension>
   <extension
         point="com.easyfun.eclipse.performance.EasyFunNavigatorElement">
      <page
            compositeClass="com.easyfun.eclipse.performance.socket.tcpclient.SWTSocketClient"
            folder="com.easyfun.eclipse.performance.socketFolder"
            icon="icons/socket.png"
            id="com.easyfun.eclipse.component.socket.tcpclient.swtSocketClient"
            title="SocketClient"
            type="SocketClient"
            visible="true">
      </page>
      <page
            compositeClass="com.easyfun.eclipse.performance.socket.tcpserver.SWTSocketServer"
            folder="com.easyfun.eclipse.performance.socketFolder"
            icon="icons/socket.png"
            id="com.easyfun.eclipse.component.socket.tcpserver.swtSocketServer"
            title="SocketServer"
            type="SocketServer"
            visible="true">
      </page>
      <page
            compositeClass="com.easyfun.eclipse.performance.socket.udp.SWTSocketUdp"
            folder="com.easyfun.eclipse.performance.socketFolder"
            icon="icons/socket.png"
            id="com.easyfun.eclipse.component.socket.udp.swtSocketUdp"
            title="UDP"
            type="UDP"
            visible="true">
      </page>
   </extension>
</plugin>
